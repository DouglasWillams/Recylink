{
  "version": 2,
  "builds": [
    {
      "src": "frontend/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
   
    {
      "src": "/api/(.*)",
      "dest": "https://<SEU_URL_PUBLICO_RAILWAY>.up.railway.app/api/$1" 
    },
    
    {
      "src": "/assets/(.*)",
      "dest": "frontend/assets/$1"
    },
    {
      "src": "/pages/(.*)",
      "dest": "frontend/pages/$1"
    },
    {
      "src": "/(.*\\.html)",
      "dest": "frontend/pages/$1"
    },
    {
      "src": "/",
      "dest": "frontend/pages/index.html"
    }
  ]
}